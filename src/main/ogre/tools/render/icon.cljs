(defn icon [{:keys [name width height]}]
  (let [child (icons (clojure.core/name name))]
    (when child
      [:svg {:xmlns   "http://www.w3.org/2000/svg"
             :class   (str "bi bi-" name)
             :width   width
             :height  height
             :fill    "currentColor"
             :viewBox "0 0 16 16"}
       child])))
